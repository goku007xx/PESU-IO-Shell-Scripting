Vagrant.configure("2") do |config|
  config.vm.box = "jasonc/centos7"
  config.vm.define "testbox2" do |testbox2|
    testbox2.vm.hostname="testbox2"
    testbox2.vm.network "private_network", ip:"10.9.8.5"
  end

  config.vm.define "testbox3" do |testbox3|
    testbox3.vm.hostname="testbox3"
    testbox3.vm.network "private_network", ip:"10.9.8.6"
  end
